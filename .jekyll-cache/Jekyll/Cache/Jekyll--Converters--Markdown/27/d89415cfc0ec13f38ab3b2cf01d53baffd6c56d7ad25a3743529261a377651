<p>#include <iostream>
#include <list></list></iostream></p>

<p>template <typename T="">
void print_list(std::list<T>&amp; lst) {
  std::cout &lt;&lt; "[ ";
  // ì „ì²´ ë¦¬ìŠ¤íŠ¸ë¥¼ ì¶œë ¥í•˜ê¸° (ì´ ì—­ì‹œ ë²”ìœ„ ê¸°ë°˜ for ë¬¸ì„ ì“¸ ìˆ˜ ìˆìŠµë‹ˆë‹¤)
  for (const auto&amp; elem : lst) {
    std::cout &lt;&lt; elem &lt;&lt; " ";
  }
  std::cout &lt;&lt; "]" &lt;&lt; std::endl;
}
int main() {
  std::list<int> lst;</int></T></typename></p>

<p>lst.push_back(10);
  lst.push_back(20);
  lst.push_back(30);
  lst.push_back(40);</p>

<p>std::cout Â«Â â€œì²˜ìŒ ë¦¬ìŠ¤íŠ¸ì˜ ìƒíƒœ â€œ Â«Â std::endl;
  print_list(lst);</p>

<p>for (std::list<int>::iterator itr = lst.begin(); itr != lst.end(); ++itr) {
    // ë§Œì¼ í˜„ì¬ ì›ì†Œê°€ 20 ì´ë¼ë©´
    // ê·¸ ì•ì— 50 ì„ ì§‘ì–´ë„£ëŠ”ë‹¤.
    if (*itr == 20) {
      lst.insert(itr, 50);
    }
  }</int></p>

<p>std::cout Â«Â â€œê°’ì´ 20 ì¸ ì›ì†Œ ì•ì— 50 ì„ ì¶”ê°€ â€œ Â«Â std::endl;
  print_list(lst);</p>

<p>for (std::list<int>::iterator itr = lst.begin(); itr != lst.end(); ++itr) {
    // ê°’ì´ 30 ì¸ ì›ì†Œë¥¼ ì‚­ì œí•œë‹¤.
    if (*itr == 30) {
      lst.erase(itr);
      break;
    }
  }</int></p>

<p>std::cout Â«Â â€œê°’ì´ 30 ì¸ ì›ì†Œë¥¼ ì œê±°í•œë‹¤â€ Â«Â std::endl;
  print_list(lst);
