<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>//using [namespace ëª…]::ì‚¬ìš©í•  ì´ë¦„; ìœ¼ë¡œ íŠ¹ì • ì´ë¦„ë§Œ ê°„ë‹¨íˆ ì‚¬ìš©
//Program.cpp
#include &lt;iostream&gt;
using namespace std;

namespace DemoLib
{
    struct Stack
    {
        int top;
    };
    struct Queue
    {
        int front;
        int rear;
    };
}

//DemoLib ë„¤ì„ ìŠ¤í˜ì´ìŠ¤ì— Stack ì´ë¦„ì„ ì‚¬ìš©í•˜ê² ë‹¤ëŠ” êµ¬ë¬¸
using DemoLib::Stack;

int main()
{
    Stack s; //ë„¤ì„ ìŠ¤í˜ì´ìŠ¤ ëª…ê³¼ ìŠ¤ì½”í”„ ì—°ì‚°ì—†ì´ ì´ë¦„ì„ ë°”ë¡œ ì‚¬ìš©
    DemoLib::Queue q; //ë„¤ì„ ìŠ¤í˜ì´ìŠ¤ ëª…ê³¼ ìŠ¤ì½”í”„ ì—°ì‚°ê³¼ í•¨ê»˜ ì´ë¦„ì„ ì‚¬ìš©
    s.top = -1;
    q.front = q.rear=0;

    return 0;
}
</code></pre></div></div>

<h2 id="example">Example</h2>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>#include &lt;iostream&gt;


/*namespaces */

namespace mySpace
{
    class MyNewLine
    {
            std::string text;
        public:
            MyNewLine(std::string text = "\n\n\n") { this-&gt;text = text;}
            std::string toString()
            {
                return text;
            }
    };

    std::ostream &amp; operator&lt;&lt;(std::ostream &amp; out, MyNewLine &amp; o)
    {
        return out &lt;&lt; o.toString();
    }
    MyNewLine endl("\n\n\n\n\n\n");
}
using namespace mySpace;
int main()
{
    int a = 50;


    std::cout &lt;&lt; "this is text: " &lt;&lt; endl;
    return 0;
}

</code></pre></div></div>
