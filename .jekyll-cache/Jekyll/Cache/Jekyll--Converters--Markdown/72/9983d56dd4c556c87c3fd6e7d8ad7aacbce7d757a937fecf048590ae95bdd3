<p>#include <iostream>
#include <exception></exception></iostream></p>

<p>using namespace std;</p>

<p>class MyException: public exception {
    //public exepction, is that inherit exception class
public:
	virtual const char* what() const throw(){
	//can not throw the exception.
	//it is the optimize the way
	//it halt the time.
		return â€œSomething bad happened!â€;
	}
};</p>

<p>class Test {
public:
	void goesWrong() {
		throw MyException();
	}
};</p>

<p>int main() {</p>

<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>Test test;

try {
	test.goesWrong();
}
catch(MyException &amp;e) {
	cout &lt;&lt; e.what() &lt;&lt; endl;
}


return 0; }
</code></pre></div></div>

